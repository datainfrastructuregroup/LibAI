{
  "name": "libai",
  "version": "1.0.0",
  "private": false,
  "description": "The new text-based Liberation + AI website.",
  "type": "module",
  "homepage": "https://datainfrastructuregroup.github.io/LibAI",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datainfrastructuregroup/LibAI.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "eleventy --serve --port=9090",
    "build": "node -e \"const { generateGraph } = require('./scripts/generate-graph.cjs'); generateGraph();\" && eleventy",
    "build:gh-pages": "npm run build",
    "generate-graph": "node -e \"const { generateGraph } = require('./scripts/generate-graph.cjs'); generateGraph();\""
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@adaptivekind/markdown-graph": "^0.0.17",
    "clean-css": "^5.3.2",
    "eleventy-plugin-citations": "^0.1.10",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "markdown-it-wikilinks": "^1.1.1"
  }
}
